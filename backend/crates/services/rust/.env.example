# Rust Verifier Service Configuration
# Copy this file to .env.<service_name> (e.g., .env.zisk, .env.sp1)

# Service identifier (e.g., zisk, sp1)
SERVICE_NAME=

# Path to artifacts directory containing manifest.yaml, binaries, and VKs
ARTIFACTS_DIR=

# gRPC port to listen on (default: 50051)
GRPC_PORT=50051

# Logging level (trace, debug, info, warn, error)
RUST_LOG=info
